<!--
  Description:  image contains url in png format
  Expect:       !ImageUrlFormat{parent:image,element:url}
-->

<rss version="2.0">
<channel>
<title>Valid image</title>
<link>http://purl.org/rss/2.0/</link>
<description>foo</description>
<image>
<title>Valid image</title>
<link>http://purl.org/rss/2.0/</link>
<url>http://example.com/image.png</url>
</image>
</channel>
</rss>
