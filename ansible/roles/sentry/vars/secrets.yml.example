---
# Copy this file to secrets.yml and fill in your actual values
# secrets.yml is gitignored and will not be committed

# Sentry public URL (e.g., https://sentry.newsblur.com)
sentry_url: "https://sentry.newsblur.com"

# Sentry system secret key (from sentry/config.yml)
# Generate with: docker run --rm getsentry/sentry config generate-secret-key
sentry_secret_key: "YOUR_SECRET_KEY_HERE"

# Relay credentials (from relay/credentials.json)
sentry_relay_secret_key: "YOUR_RELAY_SECRET_KEY_HERE"
sentry_relay_public_key: "YOUR_RELAY_PUBLIC_KEY_HERE"
sentry_relay_id: "YOUR_RELAY_ID_HERE"
