{
    "service": {
        "name": "{{ inventory_hostname|regex_replace('\d+', '') }}",
        "id": "{{ inventory_hostname }}-web",
        "tags": [
            "web"
        ],
        "port": 8000,
        "checks": [{
            "id": "{{inventory_hostname}}-web-ping",
            "http": "http://{{ ansible_ssh_host }}:8000/_haproxychk",
            "interval": "15s"
        }]
    }
}  
